{
	"name": "drago-ex/generator",
	"type": "library",
	"description": "A lightweight CLI tool for generating PHP entity and data class files from your database schema.",
	"license": ["MIT"],
	"authors": [
		{
			"name": "Zdeněk Papučík",
			"homepage": "https://github.com/accgit"
		}
	],
	"require": {
		"php": ">=8.3 <9",
		"contributte/console": "^0.10",
		"doctrine/inflector": "^2.0",
		"drago-ex/utils": "^1.0",
		"drago-ex/database": "^2.0",
		"nette/php-generator": "^4.1"
	},
	"require-dev": {
		"nette/bootstrap": "^3.2",
		"nette/tester": "^2.5",
		"tracy/tracy": "^2.10",
		"phpstan/phpstan-nette": "^1.2.9"
	},
	"bin": [
		"bin/generator"
	],
	"autoload": {
		"classmap": ["src/"]
	},
	"scripts": {
		"phpstan": "phpstan analyse --level 5 --configuration tests/phpstan.neon src"
	}
}
